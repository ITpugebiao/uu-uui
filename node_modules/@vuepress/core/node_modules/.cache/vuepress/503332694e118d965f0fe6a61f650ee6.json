#909399;\n$info-active-color: #a6a9ad;\n$success-color: #67c23a;\n$success-active-color: #85ce61;\n$warning-color: #e6a23c;\n$warning-active-color: #ebb563;\n$danger-color: #f56c6c;\n$danger-active-color: #f78989;\n\n.uu-button {\n  display: inline-block;\n  position: relative;\n  font-family: 'PingFang SC', 'Microsoft Yahei', sans-serif;\n  line-height: 1;\n  cursor: pointer;\n  background-color: $normal-color;\n  border: 1px solid #dcdfe6;\n  white-space: nowrap;\n  color: #606266;\n  box-sizing: border-box;\n  outline: none;\n  transition: 0.1s;\n  font-weight: 500;\n  // 禁止文字被选中\n  -moz-user-select: none;\n  -webkit-user-select: none;\n  -ms-user-select: none;\n  padding: 12px 20px;\n  font-size: 14px;\n  border-radius: 4px;\n  &:hover,\n  &:focus {\n    color: $normal-active-color;\n    border-color: #c6e2ff;\n    background-color: #ecf5ff;\n  }\n}\n.uu-button--primary {\n  color: #fff;\n  background-color: $primary-color;\n  border-color: $primary-color;\n  //   box-shadow: inset 3px 6px 10px #337ecc, inset -3px -7px 10px #4dbeff;\n  &:hover,\n  &:focus {\n    background: $normal-active-color;\n    background-color: $normal-active-color;\n    color: #fff;\n  }\n}\n.uu-button--success {\n  color: #fff;\n  background-color: $success-color;\n  border-color: $success-color;\n  &:hover,\n  &:focus {\n    background: $success-active-color;\n    background-color: $success-active-color;\n    color: #fff;\n  }\n}\n.uu-button--info {\n  color: #fff;\n  background-color: $info-color;\n  border-color: $info-color;\n  &:hover,\n  &:focus {\n    background: $info-active-color;\n    background-color: $info-active-color;\n    color: #fff;\n  }\n}\n.uu-button--warning {\n  color: #fff;\n  background-color: $warning-color;\n  border-color: $warning-color;\n  &:hover,\n  &:focus {\n    background: $warning-active-color;\n    background-color: $warning-active-color;\n    color: #fff;\n  }\n}\n.uu-button--danger {\n  color: #fff;\n  background-color: $danger-color;\n  border-color: $danger-color;\n  &:hover,\n  &:focus {\n    background: $danger-active-color;\n    background-color: $danger-active-color;\n    color: #fff;\n  }\n}\n// 拟态\n.uu-button.is-mimicry {\n  box-shadow: inset 3px 6px 10px #ccc, inset -3px -7px 10px #fff;\n}\n.uu-button--primary.is-mimicry {\n  box-shadow: inset 3px 6px 10px #337ecc, inset -3px -7px 10px #4dbeff;\n}\n.uu-button--info.is-mimicry {\n  box-shadow: inset 3px 6px 10px #73767a, inset -3px -7px 10px #adb0b8;\n}\n.uu-button--success.is-mimicry {\n  box-shadow: inset 3px 6px 10px #6aa54e, inset -3px -7px 10px #a0f774;\n}\n.uu-button--warning.is-mimicry {\n  box-shadow: inset 3px 6px 10px #bc914f, inset -3px -7px 10px #ffd977;\n}\n.uu-button--danger.is-mimicry {\n  box-shadow: inset 3px 6px 10px #c45656, inset -3px -7px 10px #ff8282;\n}\n// 朴素按钮\n.uu-button.is-plain {\n  box-shadow: unset;\n  &:hover,\n  &:focus {\n    background: #fff;\n    border-color: #489eff;\n    color: #409eff;\n  }\n}\n.uu-button--primary.is-plain {\n  box-shadow: unset;\n  color: #409eff;\n  background: #ecf5ff;\n  &:hover,\n  &:focus {\n    background: #409eff;\n    border-color: #409eff;\n    color: #fff;\n  }\n}\n.uu-button--success.is-plain {\n  box-shadow: unset;\n  color: #67c23a;\n  background: #c2e7b0;\n  &:hover,\n  &:focus {\n    background: #67c23a;\n    border-color: #67c23a;\n    color: #fff;\n  }\n}\n.uu-button--info.is-plain {\n  box-shadow: unset;\n  color: #909399;\n  background: #d3d4d6;\n  &:hover,\n  &:focus {\n    background: #909399;\n    border-color: #909399;\n    color: #fff;\n  }\n}\n.uu-button--warning.is-plain {\n  box-shadow: unset;\n  color: #e6a23c;\n  background: #f5dab1;\n  &:hover,\n  &:focus {\n    background: #e6a23c;\n    border-color: #e6a23c;\n    color: #fff;\n  }\n}\n.uu-button--danger.is-plain {\n  box-shadow: unset;\n  color: #f56c6c;\n  background: #fbc4c4;\n  &:hover,\n  &:focus {\n    background: #f56c6c;\n    border-color: #f56c6c;\n    color: #fff;\n  }\n}\n// round\n.uu-button.is-round {\n  border-radius: 20px;\n  padding: 12px 23px;\n}\n// 圆形按钮\n.uu-button.is-circle {\n  border-radius: 50%;\n  padding: 12px;\n}\n// 让图标和文字之间空开\n.uu-button [class*='uu-icon'] + span {\n  margin-left: 5px;\n}\n// 禁用\n.uu-button.uu-button.is-disabled .mask {\n  position: absolute;\n  width: 100%;\n  height: 100%;\n  background-color: rgba(148, 146, 146, 0.39);\n  left: 0;\n  top: 0;\n  box-shadow: 2px 2px 3px rgba(216, 213, 213, 0.3);\n  border-radius: 4px;\n  z-index: 100;\n}\n.uu-button.is-round .mask {\n  border-radius: 20px;\n}\n.uu-button.is-circle .mask {\n  border-radius: 50%;\n}\n.uu-button.is-disabled {\n  cursor: no-drop;\n}\n</style>\n"]}]}